{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "import-name": false,
    "interface-name": false,
    "ordered-imports": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"]
  }
}
